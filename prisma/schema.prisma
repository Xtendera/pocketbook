// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Book {
  uuid  String @id @default(uuid())
  title String
}

model User {
  uuid         String @id @default(uuid())
  username     String @unique
  passwordHash String
  permission   Int
}
